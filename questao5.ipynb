      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "tolerancia = 0.0000001\n",
        "eps = 1\n",
        "xo = -1.8\n",
        "x = 0\n",
        "while(eps > tolerancia):\n",
        "  x = phi(xo)\n",
        "  eps = abs(xo - x)\n",
        "  xo = x"
      ],
